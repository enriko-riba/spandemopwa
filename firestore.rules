service cloud.firestore {
	match /databases/{database}/documents {
		match /notes/{note} {
			allow read,write: if true;
			allow update: if request.resource.data.email == resource.data.email;
		}
		match /users/{user} {
			allow read, write: if true;
		}
		match /images/{image} {
			allow read, write: if true;
		}
	}
}
